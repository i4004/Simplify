<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net45</TargetFrameworks>
		<Authors>Alexander Krylkov</Authors>
		<Product>Simplify</Product>
		<Description>System.Windows.Forms additional controls</Description>
		<Copyright>Licensed under LGPL</Copyright>
		<Version>1.0.1</Version>
		<PackageProjectUrl>https://github.com/i4004/Simplify</PackageProjectUrl>
		<PackageIconUrl>https://raw.githubusercontent.com/i4004/Simplify/master/Images/Icon.png</PackageIconUrl>
		<RepositoryUrl>https://github.com/i4004/Simplify/tree/master/src/Simplify.Pipelines</RepositoryUrl>
		<RepositoryType>GIT</RepositoryType>
		<PackageTags>.NET pipelines</PackageTags>
		<PackageReleaseNotes>
		</PackageReleaseNotes>
		<OutputPath>bin\Any CPU\$(Configuration)\</OutputPath>
		<DocumentationFile>bin\Any CPU\$(Configuration)\$(TargetFramework)\Simplify.Windows.Forms.xml</DocumentationFile>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Simplify.Resources" Version="1.0.0" />
		<Reference Include="System.Windows.Forms" />
	</ItemGroup>
</Project>