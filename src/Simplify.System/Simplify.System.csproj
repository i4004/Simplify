<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net452</TargetFrameworks>
		<Authors>Alexander Krylkov</Authors>
		<Product>Simplify</Product>
		<Description>Core classes for applications and libraries</Description>
		<Copyright>Licensed under LGPL</Copyright>
		<Version>1.0.1</Version>
		<PackageProjectUrl>https://github.com/i4004/Simplify/wiki/Simplify.System</PackageProjectUrl>
		<PackageIconUrl>https://raw.githubusercontent.com/i4004/Simplify/master/Images/Icon.png</PackageIconUrl>
		<RepositoryUrl>https://github.com/i4004/Simplify/tree/master/src/Simplify.System</RepositoryUrl>
		<RepositoryType>GIT</RepositoryType>
		<PackageTags>.NET</PackageTags>
		<PackageReleaseNotes>
			* Added .NET Standard 2.0 support
			* Upgrade to .NET Framework 4.5.2
		</PackageReleaseNotes>
		<OutputPath>bin\Any CPU\$(Configuration)\</OutputPath>
		<DocumentationFile>bin\Any CPU\$(Configuration)\$(TargetFramework)\Simplify.System.xml</DocumentationFile>
	</PropertyGroup>
</Project>